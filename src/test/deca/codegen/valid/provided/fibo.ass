; start main program
	BRA debutProg
erreurDiv0:
	WSTR "Arret de l'execution: division par z√©ro impossible."
	HALT
debutProg:
; Main program
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
	ADDSP #3
	BRA debutMain
debutMain:
; Beginning of main instructions:
	LOAD #200, R2
	STORE R2, 3(GB)
	LOAD #0, R2
	STORE R2, 4(GB)
	LOAD #1, R2
	STORE R2, 5(GB)
	LOAD #0, R2
	STORE R2, 6(GB)
	LOAD #0, R2
	STORE R2, 7(GB)
	WSTR "Les 200 premiers nombres de la suite de Fibonacci sont :"
	LOAD R0, R1
debutWhile1:
	LOAD 7(GB), R2
	LOAD 3(GB), R3
	CMP R3, R2
	BGE finWhile1
	LOAD 6(GB), R2
	LOAD R2, R1
	WINT
	WSTR " "
	LOAD R0, R1
	LOAD 5(GB), R2
	STORE R2, 4(GB)
	LOAD 6(GB), R2
	STORE R2, 5(GB)
	LOAD 4(GB), R2
	LOAD 5(GB), R3
	ADD R3, R2
	STORE R2, 6(GB)
	LOAD 7(GB), R2
	LOAD #1, R3
	ADD R3, R2
	STORE R2, 7(GB)
	BRA debutWhile1
finWhile1:
	HALT
code.Object.equals:
	LOAD -2(LB), R0
	LOAD -3(LB), R1
	CMP R0, R1
	SEQ R0
	RTS
deferencement_null:
	WSTR "Erreur : deferencement_null"
	WNL
	ERROR
pile_pleine:
	WSTR "Erreur : pile_pleine"
