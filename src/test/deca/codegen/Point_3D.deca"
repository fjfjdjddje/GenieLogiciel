#include "Point.deca"
class Point_3D extends Point{
    float z=0;

    void setZ(float z){
        this.z=z;
    }

    float getZ(){
        return this.z;
    }

    void translation_3D(float a, float b,float c) {
		this.x=this.x+a;
		this.y=this.y+b;
        this.z=this.z+c;
	}

    void toString(){
        println("le point a les coordonn√©es: (",x,",",y,",",z,")");
    }

}

{   

    Point_3D p3= new Point_3D();
    p3.toString();
    p3.translation_3D(5.0,8.3,-6.4);
    p3.toString();

}