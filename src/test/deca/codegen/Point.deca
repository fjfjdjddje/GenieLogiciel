class Point{
    float x=0;
    float y=0;
    
    void setX(float x){
        this.x=x;
    }

    float getX(){
        return this.x;
    }

    void setY(float x){
        this.y=x;
    }

    float getY(){
        return this.y;
    }


    void translation_2D(float a, float b) {
		this.x=this.x+a;
		this.y=this.y+b;
	}

}